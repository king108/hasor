<?xml version="1.0" encoding="UTF-8"?>
<config xmlns="http://project.hasor.net/hasor/schema/main">
    <hasor.environmentVar>

        <!-- 工作目录 -->
        <WORK_HOME>%USER.HOME%/hasor-work/rsf-center-alone</WORK_HOME>
        <!-- 禁用磁盘地址缓存 -->
        <RSF_DISK_CACHE_ENABLE>false</RSF_DISK_CACHE_ENABLE>

        <!-- 注册中心工作模式 -->
        <RSF_CENTER_WORK_MODE>server</RSF_CENTER_WORK_MODE>
        <!-- RSF对外提供服务的IP -->
        <RSF_SERVICE_IP>127.0.0.1</RSF_SERVICE_IP>
        <!-- Telnet 控制台端口 -->
        <RSF_CONSOLE_PORT>2180</RSF_CONSOLE_PORT>
        <!-- RSF对外提供服务的服务端口（可以配置 0 用来关闭 RSF连接器） -->
        <RSF_SERVICE_PORT>2181</RSF_SERVICE_PORT>

    </hasor.environmentVar>
</config>