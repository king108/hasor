env=pre
# 默认加载的包
HASOR_LOAD_PACKAGES=net.*,org.*,com.*
# WebMVC 拦截的请求类型
HASOR_RESTFUL_PATTERNS=htm;html;do;json;
# 程序入口模块类名
HASOR_LOAD_STARTUP=
# 执行事件的线程池大小
HASOR_LOAD_EVENT_POOL=20
# Fileupload 允许的请求大小
HASOR_UPLOAD_MAX_REQUEST_SIZE=-1
# Fileupload 允许上传的单个文件大小
HASOR_UPLOAD_MAX_FILE_SIZE=-1
# 工作目录
WORK_HOME=